//080
//A3
#include <iostream>
using namespace std;

int main() {
    int n, p, r, s;

    
    cout << "Enter rows and columns of matrix A: ";
    cin >> n >> p;

    cout << "Enter rows and columns of matrix B: ";
    cin >> r >> s;

    
    if (p != r) {
        cout << "Matrix multiplication not possible! Columns of A must equal rows of B.\n";
        return 0;
    }

    
    int a[n][p], b[r][s], product[n][s];

    
    cout << "Enter elements of matrix A:\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < p; j++) {
            cin >> a[i][j];
        }
    }

    
    cout << "Enter elements of matrix B:\n";
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < s; j++) {
            cin >> b[i][j];
        }
    }

    
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < s; j++) {
            product[i][j] = 0;
        }
    }

    
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < s; j++) {
            for (int k = 0; k < p; k++) {
                product[i][j] += a[i][k] * b[k][j];
            }
        }
    }

    
    cout << "Product of the two matrices:\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < s; j++) {
            cout << product[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}

OUTPUT:
Enter rows and columns of matrix A: 2 3
Enter rows and columns of matrix B: 3 1
Enter elements of matrix A:
2 3
3 4
5 6
Enter elements of matrix B:
6 5 4
Product of the two matrices:
39 
73 


